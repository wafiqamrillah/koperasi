{
    "name": "jcergolj/laravellte",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.0",
        "laravel/ui": "^4.2",
        "livewire/livewire": "^2.10",
        "mediconesystems/livewire-datatables": "dev-master",
        "predis/predis": "^1.1",
        "reedware/laravel-relation-joins": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.18",
        "laravel/tinker": "^2.7",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "*",
        "nunomaduro/phpinsights": "^2.1",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        },
        "hooks": {
            "pre-commit": [
                "STAGED_FILES=$(git diff --name-only --cached)",
                "vendor/bin/php-cs-fixer fix",
                "for FILE in $STAGED_FILES ; do git add $FILE; done",
                "php artisan insights --no-interaction --min-quality=80 --min-complexity=80 --min-architecture=80 --min-style=80"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": ["app/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/helpers.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "cghooks": "vendor/bin/cghooks",
        "sniff": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
       ],
       "lint": [
           "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
       ]
    }
}
